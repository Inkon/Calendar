<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
    <g:HTMLPanel>
        <b:Modal title="Импортировать календарь" closable="true" fade="true" dataBackdrop="STATIC" dataKeyboard="true"
                 b:id="importCalendarModal" ui:field="importCalendarModal">
            <b:ModalBody ui:field="modalBody">

            </b:ModalBody>
            <b:ModalFooter>
                <b:Button type="PRIMARY" ui:field="sendForm" dataLoadingText="Загрузка...">Загрузить</b:Button>
                <b:Button type="DANGER" ui:field="closeModal" dataDismiss="MODAL">Отмена</b:Button>
            </b:ModalFooter>
        </b:Modal>
    </g:HTMLPanel>
</ui:UiBinder>